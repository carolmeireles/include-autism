<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout9"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#5B00B7FF">

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/imgprincipal_foreground" />

    <TextView
        android:id="@+id/txtCodSenha"
        android:layout_width="0dp"
        android:layout_height="37dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="17dp"
        android:text="Senha temporÃ¡ria:"
        app:layout_constraintBaseline_toBaselineOf="@+id/et_senhaTemp"
        app:layout_constraintEnd_toStartOf="@+id/et_senhaTemp"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/txtNovaSenha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="23dp"
        android:text="Nova Senha:"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtCodSenha" />

    <EditText
        android:id="@+id/et_senhaNova"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="1dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/et_senhaTemp"
        app:layout_constraintTop_toBottomOf="@+id/et_senhaTemp" />

    <Button
        android:id="@+id/bt_salvarSenha"
        android:layout_width="85dp"
        android:layout_height="37dp"
        android:layout_marginTop="29dp"
        android:background="#FDFDFD"
        android:text="Salvar"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_senhaNova" />

    <EditText
        android:id="@+id/et_senhaTemp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="30dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/txtCodSenha"
        app:layout_constraintTop_toBottomOf="@+id/imageView12" />
</androidx.constraintlayout.widget.ConstraintLayout>